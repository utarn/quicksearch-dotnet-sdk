<Project>
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    
    <!-- Package Metadata -->
    <Authors>QuickSearch Contributors</Authors>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/your-org/quicksearch-dotnet-sdk</PackageProjectUrl>
    <RepositoryUrl>https://github.com/your-org/quicksearch-dotnet-sdk</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Copyright>Copyright Â© 2024 QuickSearch Contributors</Copyright>
  </PropertyGroup>
</Project>